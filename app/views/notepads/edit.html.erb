<div class="container">
  <div class="row">
    <div class="col-lg-12">
      <hr>
      <h1 class="page-header">Edit Notepad
      </h1>
    </div>
  </div>

  <div>
    <%= form_for :notepad, url: notepad_path(@notepad), method: :patch do |f| %>
    <%= f.text_field :title %>
    <%= f.submit "Create", :class => 'btn btn-primary' %>
    <% end %>
  </div>
</div>
